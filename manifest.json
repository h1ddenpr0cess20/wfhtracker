{
  "manifest_version": 3,
  "name": "WFH Tracker",
  "version": "1.0",
  "description": "A simple time tracking tool.",
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_title": "WFH Tracker"
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
